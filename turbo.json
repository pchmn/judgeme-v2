{
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": ["dist/**", "build/**", ".expo/**", "node_modules/.cache/metro/**"],
      "dependsOn": ["^build"]
    },
    "@kuzpot/functions#deploy:dev": {
      "dependsOn": ["@kuzpot/functions#build"]
    },
    "@kuzpot/functions#deploy:prod": {
      "dependsOn": ["@kuzpot/functions#build"]
    }
  }
}
